    "vue": "^2.2.1"
  },
  "devDependencies": {
    "concurrently": "^3.3.0",
    "electron-reload": "^1.1.0",
    "rollup": "^0.41.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-watch": "^3.2.2",
    "typescript": "^2.2.1"
  }
